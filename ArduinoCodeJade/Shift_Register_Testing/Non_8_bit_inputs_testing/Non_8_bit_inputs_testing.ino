// inslude the SPI library:#include <SPI.h>// set pin 10 as the slave select for the digital pot:const int slaveSelectPin = 10;int bits = 0b0100000010011111;  // sends in groups of 8 bits//                   /       /       // sends frist 8 bits, then sends second 8 bits                                    // if less then 8 bits you get sent                                    /*int bits = 0b01000000111111110000; //first 4 outputs are high//                   /       /      int bits = 0b010000001111111100000000; // no leds onint bits = 0b01000000111111110000000;  // just Q0 output highint bits = 0b0100000010011111; // outputs are high: 0, 3, 4, 5, 6, 7*/void setup() {    Serial.begin(9600);  // set the slaveSelectPin as an output:  pinMode(slaveSelectPin, OUTPUT);  // initialize SPI:  SPI.begin();  Serial.println("Start:");  SPI.setBitOrder(LSBFIRST);}void loop() {    writebits();    delay(1000);}void writebits(){    digitalWrite(slaveSelectPin, LOW);    SPI.transfer(bits);    digitalWrite(slaveSelectPin, HIGH);}